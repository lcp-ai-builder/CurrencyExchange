{
  "cSpell.enabled": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    ".config": ".npmrc,.eslintrc,.prettierrc,.babelrc,.gitignore,.gitattributes,.watchmanconfig,.stylelintrc,.env,.env.*,Gemfile,babel*,*.js,*.json,*.md",
    ".other": "LICENSE,README*",
    ".shell": "*.sh"
  },
  "explorer.sortOrder": "type",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.bundle": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/Thumbs.db": true,
    "**/__tests__": true,
    "**/android": true,
    "**/ios": true,
    "**/node_modules": true
  },
  "git.enableCommitSigning": true,
  "workbench.iconTheme": "catppuccin-perfect-dark",
  "workbench.colorTheme": "Base16 Dark Default"
}
